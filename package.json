{
  "name": "tokyo-boyz-refactor",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run build:tailwind && vite --host 0.0.0.0 --port 3000",
    "build": "npm run build:tailwind && vite build && cp dist/index.html dist/404.html",
    "preview": "vite preview",
    "start": "sirv dist preview --host 0.0.0.0 --port 3000 --single",
    "build:tailwind": "tailwindcss -i ./src/index.css -o ./src/tailwind-output.css --content './src/**/*.{js,jsx,ts,tsx}' --minify"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "framer-motion": "^11.18.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.542.0",
    "node-fetch": "^2.7.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.12.0",
    "react-leaflet": "^4.2.1",
    "sirv-cli": "^3.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.9",
    "vite": "^5.4.19",
    "vite-plugin-pwa": "^1.0.3",
    "workbox-window": "^7.3.0"
  }
}
